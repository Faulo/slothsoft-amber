{
    "name": "slothsoft/amber",
    "description": "A Farah module for processing game files from the amber engine (i.e., Ambermoon and Amberstar).",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "slothsoft/core": "*",
        "slothsoft/farah": "*",
        "slothsoft/savegame": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "slothsoft/core": "dev-master",
        "slothsoft/farah": "dev-master",
        "slothsoft/lang": "dev-master",
        "slothsoft/savegame": "dev-master",
        "slothsoft/w3c": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Amber\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "farah-app",
        "farah-module",
        "slothsoft"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false
}
