{
	"name" : "slothsoft/amber",
	"description" : "A Farah module for processing game files from the amber engine (i.e., Ambermoon and Amberstar).",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : "^8.0",
		"slothsoft/core" : "^1.14.17",
		"slothsoft/farah" : "^1.26.28",
		"slothsoft/savegame" : "^1.3.8",
		"ext-imagick" : "*",
		"ext-gd" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.6.23",
		"slothsoft/farah-testing" : "^1.2.0"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Amber\\" : "src"
		},
		"files" : [
			"src/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Amber\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.1.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/amber",
	"config" : {
		"process-timeout" : 3600
	}
}