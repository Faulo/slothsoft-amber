{
	"name" : "slothsoft/amber",
	"description" : "A Farah module for processing game files from the amber engine (i.e., Ambermoon and Amberstar).",
	"license" : "WTFPL",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : "7.4 - 8.0",
		"slothsoft/core" : "^1.5.1",
		"slothsoft/farah" : "^1.6.1",
		"slothsoft/savegame" : "^1.0.2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"suggest" : {
		"ext-imagick" : "Needed to extract images with ambgfx"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Amber\\" : "src"
		},
		"files" : [
			"scripts/registerModule.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Amber\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.1.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/amber"
}