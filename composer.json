{
	"name" : "slothsoft/amber",
	"description" : "A Farah module for processing game files from the amber engine (i.e., Ambermoon and Amberstar).",
	"license" : "WTFPL",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : "^7.2",
		"slothsoft/core" : "dev-master",
		"slothsoft/farah" : "dev-master",
		"slothsoft/savegame" : "dev-master"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6.5"
	},
	"suggest" : {
		"ext-imagick" : "Needed to extract images with ambgfx"
	},
	"autoload" : {
		"psr-0" : {
			"Slothsoft\\Amber\\" : "src/"
		},
		"files" : [
			"src/registerModule.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev" : {
		"classmap" : [
			"tests/"
		]
	},
	"classmap-authoritative" : false,
	"minimum-stability" : "dev",
	"prefer-stable" : true
}