<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns="http://schema.slothsoft.net/farah/dictionary" xmlns:sfd="http://schema.slothsoft.net/farah/dictionary" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:saa="http://schema.slothsoft.net/amber/amberdata"    xmlns:sfm="http://schema.slothsoft.net/farah/module" xmlns:sse="http://schema.slothsoft.net/savegame/editor">    <xsl:template match="/saa:amberdata">        <dictionary version="1.0">            <xsl:for-each select=".//saa:dictionary">                <xsl:variable name="dictionary" select="@dictionary-id" />                <xsl:comment>                    <xsl:value-of select="$dictionary" />                </xsl:comment>                <xsl:for-each select="saa:option">                    <sfd:text>                        <xsl:attribute name="xml:id"><xsl:value-of select="concat($dictionary, '.', @key)" /></xsl:attribute>                        <xsl:value-of select="@val" />                    </sfd:text>                </xsl:for-each>            </xsl:for-each>        </dictionary>    </xsl:template></xsl:stylesheet>